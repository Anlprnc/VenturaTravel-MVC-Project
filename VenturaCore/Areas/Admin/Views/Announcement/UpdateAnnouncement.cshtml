@model AnnouncementUpdateDto
@{
    ViewData["Title"] = "UpdateAnnouncement";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h1>Announcemenet Update Page</h1>

<form method="post">
    <input type="hidden" asp-for="AnnouncementID">
    <label>Announcemenet Title</label>
    <input type="text" asp-for="Title" class="form-control" />
    <br>
    <label>Announcemenet Content</label>
    <input type="text" asp-for="Content" class="form-control" />
    <br>
    <button class="btn btn-outline-primary">Update Announcemenet</button>
</form>